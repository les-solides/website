{
  "name": "les-solides",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve --hot",
    "build": "vue-cli-service build",
    "test:unit": "npm run lint && vue-cli-service test:unit",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "@oberon-amsterdam/horizontal": "^1.0.3",
    "axios": "^0.19.2",
    "core-js": "^3.6.5",
    "intersection-observer": "^0.11.0",
    "lodash": "^4.17.19",
    "numbro": "^2.3.1",
    "shopify-buy": "^2.10.0",
    "tailwindcss": "^1.4.6",
    "vue": "^2.6.11",
    "vue-fullpage.js": "^0.1.6",
    "vue-router": "^3.2.0",
    "vue-toasted": "^1.1.28",
    "vue2-smooth-scroll": "^1.5.0",
    "vuescroll": "^4.16.1",
    "vuex": "^3.4.0"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~4.4.0",
    "@vue/cli-plugin-eslint": "~4.4.0",
    "@vue/cli-plugin-router": "^4.4.3",
    "@vue/cli-plugin-unit-jest": "^4.4.3",
    "@vue/cli-plugin-vuex": "^4.4.3",
    "@vue/cli-service": "~4.4.0",
    "@vue/test-utils": "^1.0.3",
    "babel-eslint": "^10.1.0",
    "dotenv-webpack": "^2.0.0",
    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^6.2.2",
    "sass": "^1.26.8",
    "sass-loader": "^8.0.2",
    "vue-template-compiler": "^2.6.11"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true,
      "jest": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {
      "no-mixed-spaces-and-tabs": 0,
      "no-unused-vars": 0
    },
    "overrides": [
      {
        "files": [
          "**/__tests__/*.{j,t}s?(x)",
          "**/tests/unit/**/*.spec.{j,t}s?(x)"
        ],
        "env": {
          "jest": true
        }
      }
    ]
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}
